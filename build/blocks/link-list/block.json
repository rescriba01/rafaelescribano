{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 3,
  "name": "re/link-list",
  "version": "1.0.0",
  "title": "Link List",
  "category": "widgets",
  "icon": "admin-links",
  "description": "Display a list of links with hover effects.",
  "attributes": {
    "title": {
      "type": "string",
      "default": "Selected Projects"
    },
    "links": {
      "type": "array",
      "default": []
    },
    "className": {
      "type": "string"
    },
    "metadata": {
      "type": "object"
    },
    "introText": {
      "type": "string",
      "source": "attribute",
      "selector": "[data-intro-text]",
      "attribute": "data-intro-text"
    },
    "introLinks": {
      "type": "string",
      "source": "attribute",
      "selector": "[data-intro-links]",
      "attribute": "data-intro-links"
    }
  },
  "supports": {
    "html": false
  },
  "textdomain": "re",
  "editorScript": "file:./index.js",
  "style": "file:./style.css",
  "editorStyle": "file:./editor.css"
}